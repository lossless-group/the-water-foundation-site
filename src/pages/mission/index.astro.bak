---
import BaseThemeLayout from "../../layouts/BaseThemeLayout.astro";
import OurMissionJumbo from "../../layouts/sections/core-messages/OurMission--Jumbo.astro";

const title = "Our Mission";
const description = "Catalyze the capital stack for innovation and impact on global water resilience";
const ogImage = "/og_image--The-Water-Foundation_vnt1.jpg";
---

<BaseThemeLayout 
  title={title}
  description={description}
  themeClass="theme-water" 
  containerClass="max-w-6xl mx-auto"
  ogImage={ogImage}
  ogType="website"
>
  <OurMissionJumbo />
</BaseThemeLayout>
